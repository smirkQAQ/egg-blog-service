{
  "name": "blog_server",
  "version": "4.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true,
    "tscompiler": "ts-node/register/transpile-only"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-teggtest",
    "stop": "egg-scripts stop --title=egg-server-teggtest",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug TS_NODE_TRANSPILE_ONLY=true",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "crypto": "^1.0.1",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-cos": "^1.0.2",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "reflect-metadata": "^0.1.13",
    "stream-wormhole": "^1.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "lc",
  "license": "MIT"
}
